{
  "compilerOptions": {
    "target": "es2022",
    "lib": [
      "es2023"
    ],
    "module": "CommonJS",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "noEmitOnError": false,
    "noImplicitOverride": false,
    "noImplicitReturns": true,
    "pretty": true,
    "removeComments": false,
    "types": [
      "node"
    ]
  }
}
